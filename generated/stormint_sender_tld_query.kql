// Generated by StormInt TLD Query Generator

let suspicious_tlds = dynamic([".beauty", ".app", ".help", ".sbs", ".ink", ".cyou", ".store", ".world", ".monster", ".info", ".cloud", ".website", ".art", ".qpon", ".lat", ".work", ".shop", ".yoga", ".lol", ".digital", ".homes", ".space", ".biz", ".life", ".online", ".site", ".rip", ".blog", ".now", ".tech", ".studio", ".mobi", ".live", ".top", ".vip", ".food", ".pro", ".rent", ".click", ".club", ".xyz", ".cfd", ".fun", ".bond", ".casa", ".asia", ".one", ".icu", ".link", ".buzz"]);
EmailEvents
| where SenderFromDomain has_any (suspicious_tlds)
| project TimeGenerated, Subject, SenderFromAddress, RecipientEmailAddress, SenderFromDomain
| summarize count() by Subject, SenderFromAddress, RecipientEmailAddress, SenderFromDomain
| order by count_ desc
