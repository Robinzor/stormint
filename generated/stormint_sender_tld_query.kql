// Generated by StormInt TLD Query Generator

let suspicious_tlds = dynamic([".lat", ".club", ".top", ".work", ".tax", ".tech", ".date", ".xyz", ".bio", ".help", ".online", ".bond", ".cloud", ".icu", ".beer", ".website", ".digital", ".cyou", ".bet", ".click", ".one", ".buzz", ".art", ".link", ".shop", ".site", ".vip", ".sbs", ".mobi", ".world", ".xn--d1acj3b", ".store", ".casa", ".space", ".live", ".biz", ".qpon", ".makeup", ".app", ".info", ".asia", ".lol", ".fun", ".blog", ".autos", ".dev", ".pro", ".life", ".mom", ".cfd"]);
EmailEvents
| where SenderFromDomain has_any (suspicious_tlds)
| project TimeGenerated, Subject, SenderFromAddress, RecipientEmailAddress, SenderFromDomain
| summarize count() by Subject, SenderFromAddress, RecipientEmailAddress, SenderFromDomain
| order by count_ desc
