let suspicious_tlds = dynamic([".blog", ".bet", ".bio", ".makeup", ".work", ".mom", ".tech", ".app", ".autos", ".dev", ".qpon", ".one", ".digital", ".art", ".buzz", ".cloud", ".club", ".world", ".click", ".space", ".bond", ".cfd", ".store", ".pro", ".sbs", ".biz", ".top", ".live", ".lol", ".shop", ".mobi", ".asia", ".xyz", ".beer", ".help", ".fun", ".site", ".cyou", ".info", ".vip", ".website", ".link", ".xn--d1acj3b", ".casa", ".icu", ".online", ".tax", ".date", ".life", ".lat"]);
EmailEvents
| where SenderFromDomain has_any (suspicious_tlds)
| project TimeGenerated, Subject, SenderFromAddress, RecipientEmailAddress, SenderFromDomain
| summarize count() by Subject, SenderFromAddress, RecipientEmailAddress, SenderFromDomain
| order by count_ desc
